@model IEnumerable<CarPartsPatron.Models.PartSetup>

@{
    ViewData["Title"] = "Index";
}

<h1>Your Setups</h1>

<p>
    <a asp-action="Create">Add a Setup to a Part</a>
</p>
<table class="table">
    <thead>
        <tr>
            @*<th>
                Car
            </th>*@
            <th>
                Part Type
            </th>
            <th>
                Setup Note
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreateDateTime)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            @*<td>
                @Html.DisplayFor(modelItem => item.Car.Model)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.Part.PartType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SetupNote)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateDateTime)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
}
    </tbody>
</table>
